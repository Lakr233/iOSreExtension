{
	"name": "wikiqaqiosre",
	"publisher": "Lakr233",
	"repository": "https://github.com/Co2333/iOSreExtension",
	"displayName": "iOSre Extension Linker",
	"description": "A fast and elegant extension for VSCode used for iOSre projects. (READ README BEFORE INSTALL)",
	"license": "SEE LICENSE FILE",
	"version": "1.1.8",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:iosreIDtabSectioniDevices"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.iOSreAction-ShowVersion",
				"title": "iOSreAction-ShowVersion"
			},
			{
				"command": "iosreIDtabSectioniDevices.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "res/reload.svg",
					"dark": "res/reload.svg"
				}
			},
			{
				"command": "iosreIDtabSectionApplications.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "res/reload.svg",
					"dark": "res/reload.svg"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "iosreIDtabbarEntry",
					"title": "iOSre",
					"icon": "res/icon.svg"
				}
			]
		},
		"views": {
			"iosreIDtabbarEntry": [
				{
					"id": "iosreIDtabSectioniDevices",
					"name": "iDevices",
					"when": ""
				},
				{
					"id": "iosreIDtabSectionApplications",
					"name": "Applications",
					"when": ""
				},
				{
					"id": "iosreIDtabSectionToolboxs",
					"name": "Toolbox",
					"when": ""
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "iosreIDtabSectioniDevices.refreshEntry",
					"when": "view == iosreIDtabSectioniDevices",
					"group": "navigation"
				},
				{
					"command": "iosreIDtabSectionApplications.refreshEntry",
					"when": "view == iosreIDtabSectionApplications",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"adm-zip": "^0.4.14",
		"await-semaphore": "^0.1.3"
	}
}
